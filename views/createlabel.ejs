<form style="margin-left: 10cm;margin-top: 2cm" action="/label/create/" method="POST" class="form-inline">
  <div class="form-group mx-sm-3 mb-2">
    <label for="inputEmail1" class="sr-only">Label</label>
    <input name="label" type="text" class="form-control" id="formGroupExampleInput" placeholder="Label">
  </div>
  <button type="submit" class="btn btn-dark mb-2">Create Label</button>
</form>
<div style="margin-left: 5cm">
<table class="table" id="example">
  <thead style="background-color: #343a40;color:white">
    <tr >
      <th scope="col">No</th>
      <th scope="col">Label</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
      <% var i=0; %>
       <% locals.arr.forEach(function(x){ %>
        <tr>
          <th scope="row"><%= ++i%></th>
          <td><%=x%></td>
          <td><a class="btn btn-danger" onclick="getConfirmation('<%=x%>')"  role="button">Delete</a></td>
        </tr>
        <% }) %>
      </tbody>
</table>
</div>
        
